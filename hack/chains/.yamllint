---

extends: relaxed

rules:
  document-start: disable
  indentation:
    spaces: consistent
    indent-sequences: whatever
  line-length:
    max: 120
    allow-non-breakable-words: true
    allow-non-breakable-inline-mappings: true
  trailing-spaces:
    level: warning
  new-line-at-end-of-file:
    level: warning
